$(function() {
	var $ad_header = $('#ad_header');
	var $plugin_menu = $('#plugin_menu');
	$('#header_menu').click(function() {
		var visible = $(this).toggleClass('selected').hasClass('selected')? 'hidden' : 'visible';
		$ad_header.css('visibility', visible);
		$plugin_menu.slideToggle('fast');
	});
});
